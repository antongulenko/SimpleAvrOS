void bgx1_drawBitmap(uint8_t width, uint8_t height, const uint8_t bitmap[]) {
  const uint8_t * data = bitmap;
  uint8_t row_size = ((width-1)/8+1);
  uint8_t rows_per_tile = 18/row_size;
  uint8_t x = bgx1_y;
  uint8_t y = bgx1_x;
  while (height) {
    bgx1_move(x, y);
    uint8_t h = (height>rows_per_tile)?rows_per_tile:height;
    bgx1_drawTile(width, h, data);
    while (h--) {
      data += row_size;
      height--;
      y++;
    }
  }
}

void bgx1_drawBitmap_P(uint8_t width, uint8_t height, const prog_char * bitmap) {
  const prog_char * data = bitmap;
  uint8_t row_size = ((width-1)/8+1);
  uint8_t rows_per_tile = 18/row_size;

  uint8_t y = bgx1_x;
  while (height) {
    uint8_t h = (height>rows_per_tile)?rows_per_tile:height;
    bgx1_drawTile_P(width, h, data);
    while (h--) {
      data += row_size;
      height--;
      y++;
    }
  }
}

