  memcpy (i2c_buf+4, bitmap, size);
  bgx1_transaction_posres(4+size, 3);
}


static void bgx1_drawTile_P(uint8_t width, uint8_t height, const prog_char * bitmap) {
  i2c_buf[0] = (11<<1);
  i2c_buf[1] = CMD_Bitmap;
  i2c_buf[2] = width;
