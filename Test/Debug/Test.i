void setupMotors(uint16_t intialValue);
void testMotorsSmooth();

int main(void) {
 setupMotors(1024);
 testMotorsSmooth();
}

void setupMotors(uint16_t initialValue) {
 (*(volatile uint8_t *)((0x0A) + 0x20)) = 0xFF;
 (*(volatile uint8_t *)((0x07) + 0x20)) = 0xFF;

 (*(volatile uint8_t *)((0x08) + 0x20)) = (1 << 4) | (1 << 2);

 (*(volatile uint16_t *)(0x88)) = initialValue;
 (*(volatile uint16_t *)(0x8A)) = initialValue;

 (*(volatile uint8_t *)(0x80)) = (1 << 7) | (1 << 5) |
   (1 << 1) |
   (1 << 0);
 (*(volatile uint8_t *)(0x81)) = (1 << 0);
}

void testMotorsSmooth() {
 int up = 1;
 uint16_t a = 922;
 uint16_t b = 922;
 while (1) {
  if (up) {
   a--; b--;
  } else {
   a++; b++;
  }
  (*(volatile uint16_t *)(0x88)) = a;
  (*(volatile uint16_t *)(0x8A)) = b;
  _delay_ms(1);
  if (a > 922) {
   up = 1; a = b = 922;
  }
  if (a < 410) {
   up = 0; a = b = 410;
  }
 }
}

void testTankLedsBlink() {
 (*(volatile uint8_t *)((0x01) + 0x20)) = 0xFF;
 (*(volatile uint8_t *)((0x0A) + 0x20)) = 0xFF & ~(1 << 7);
 while (1) {
  (*(volatile uint8_t *)((0x02) + 0x20)) ^= 0xFF;
  (*(volatile uint8_t *)((0x0B) + 0x20)) = ((*(volatile uint8_t *)((0x0B) + 0x20)) ^ 0xFF) & ~(1 << 7);
  _delay_ms(1000);
 }
}

void testTankLeds() {
 (*(volatile uint8_t *)((0x01) + 0x20)) = 0xFF;
 (*(volatile uint8_t *)((0x0A) + 0x20)) = 0xFF & ~(1 << 7);
 while (1) {
  (*(volatile uint8_t *)((0x02) + 0x20)) = 0xFF;
  (*(volatile uint8_t *)((0x0B) + 0x20)) = 0xFF & ~(1 << 7);
 }
}

void testTankButtons() {
 (*(volatile uint8_t *)((0x01) + 0x20)) = 0;
 (*(volatile uint8_t *)((0x04) + 0x20)) = 0xFF;
 (*(volatile uint8_t *)((0x0A) + 0x20)) = 0xFF & ~(1 << 7);
 (*(volatile uint8_t *)((0x07) + 0x20)) = 0;
 (*(volatile uint8_t *)((0x02) + 0x20)) = 0;
 (*(volatile uint8_t *)((0x0B) + 0x20)) = 0 | (1 << 7);
 (*(volatile uint8_t *)((0x08) + 0x20)) = (1 << 2) | (1 << 3) | (1 << 4)
    | (1 << 5);
 while (1) {
  uint8_t btns = 0;
  uint8_t pinc = (*(volatile uint8_t *)((0x06) + 0x20));
  uint8_t pind = (*(volatile uint8_t *)((0x09) + 0x20));
  if (!(pinc & (1 << 5)))
   btns |= (1 << 0);
  if (!(pinc & (1 << 4)))
   btns |= (1 << 1);
  if (!(pinc & (1 << 3)))
   btns |= (1 << 2);
  if (!(pinc & (1 << 2)))
   btns |= (1 << 3);
  if (!(pind & (1 << 7)))
   btns |= (1 << 4);

  (*(volatile uint8_t *)((0x0B) + 0x20)) = btns | (1 << 7);
 }
}
